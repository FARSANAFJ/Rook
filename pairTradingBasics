        "plt.figure(figsize=(10, 6))\n",
        "plt.plot(merged_data.index, merged_data['Spread'], label='Spread', color='blue')\n",
        "plt.plot(merged_data.index, merged_data['Upper_Threshold'], label='Upper Threshold', linestyle='--', color='red')\n",
        "plt.plot(merged_data.index, merged_data['Lower_Threshold'], label='Lower Threshold', linestyle='--', color='green')\n",
        "plt.title('Spread and Thresholds between Stock A and Stock B')\n",
        "plt.xlabel('Date')\n",
        "plt.ylabel('Spread')\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()"
