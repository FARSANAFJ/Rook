      "cell_type": "code",
      "source": [
        "# Calculate Mean and Standard Deviation of the Spread\n",
        "rolling_window = 20  # Example: 20-day rolling window\n",
        "merged_data['Spread_Mean'] = merged_data['Spread'].rolling(window=rolling_window).mean()\n",
        "merged_data['Spread_Std'] = merged_data['Spread'].rolling(window=rolling_window).std()\n"
      ],
      "metadata": {
        "id": "mLjGZTnKZ_yQ"
      },
      "execution_count": 13,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Plotting the Mean and Standard Deviation of the Spread\n",
        "plt.figure(figsize=(10, 6))\n",
        "\n",
        "# Plotting the Spread\n",
        "plt.plot(merged_data.index, merged_data['Spread'], label='Spread', color='blue')\n",
        "\n",
        "# Plotting the Mean\n",
        "plt.plot(merged_data.index, merged_data['Spread_Mean'], label='Mean', color='red', linestyle='--')\n",
        "\n",
        "# Plotting the Upper and Lower Thresholds (optional)\n",
        "threshold_multiplier=2\n",
        "plt.plot(merged_data.index, merged_data['Spread_Mean'] + threshold_multiplier * merged_data['Spread_Std'],\n",
        "         label='Upper Threshold', color='green', linestyle='--')\n",
        "plt.plot(merged_data.index, merged_data['Spread_Mean'] - threshold_multiplier * merged_data['Spread_Std'],\n",
        "         label='Lower Threshold', color='orange', linestyle='--')\n",
        "\n",
        "plt.title('Spread, Mean, and Standard Deviation')\n",
        "plt.xlabel('Date')\n",
        "plt.ylabel('Spread')\n",
        "plt.axhline(y=0, color='gray', linestyle='--')  # Add a horizontal line at y=0 for reference\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()"
      ],
      "metadata": {
