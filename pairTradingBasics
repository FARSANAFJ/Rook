    {
      "cell_type": "code",
      "source": [
        "\n",
        "merged_data['Signal'] = np.where(merged_data['Spread'] > merged_data['Upper_Threshold'], -1,\n",
        "                                np.where(merged_data['Spread'] < merged_data['Lower_Threshold'], 1, 0))\n"
      ],
