    {
      "cell_type": "code",
      "source": [
        "# Define Thresholds\n",
        "threshold_multiplier = 2  # Example: 2 standard deviations\n",
        "merged_data['Upper_Threshold'] = merged_data['Spread_Mean'] + threshold_multiplier * merged_data['Spread_Std']\n",
        "merged_data['Lower_Threshold'] = merged_data['Spread_Mean'] - threshold_multiplier * merged_data['Spread_Std']"
      ],
      "metadata": {
        "id": "c7meBpIi1K0t"
      },
      "execution_count": 16,
